{
    "manifest_version": 3,
    "name": "Quickreader",
    "description": "Quickreader is the best extension to create and manage a web reading list.",
    "permissions":[
        "storage",
        "tabs",
        "favicon"
    ],
    "background": {
        "service_worker": "src/js/background.js"
    },
    "version": "1.0",
    "icons": {
      "128": "src/logo128.png"
    },
    "action": {
      "default_popup": "src/html/popup.html",
      "default_icon": "src/popup.png"
    }
  }
